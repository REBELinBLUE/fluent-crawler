{
    "name": "rebelinblue/fluent-web-crawler",
    "description": "A web crawler with a fluent interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Ball",
            "email": "stephen.ball@sainsburys.co.uk"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.4.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-parallel-lint/php-console-highlighter": "^0.4",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "larapack/dd": "^1.1",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "REBELinBLUE\\Crawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "REBELinBLUE\\Crawler\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
